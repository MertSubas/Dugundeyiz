<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

<style>
    .swiper {
        width: 100%;
        height: 100%;
    }

    .swiper-slide {
        text-align: center;
        font-size: 18px;
        background: #fff;
        display: flex;
        justify-content: center;
        align-items: center;
        display: flex;
        flex-direction: column;
    }

    .subCategoriesCDropdown {
        height: 35px;
        margin-bottom: 9px !important;
    }

    .custom-card-123 .mt-3 {
        display: none;
    }

    .dropdownUlItems {
        display: none;
        position: absolute;
        background: #ece4d4;
        border: 1px solid rgb(204, 204, 204);
        border-radius: 8px;
        list-style-type: none;
        z-index: 10;
        padding: 5px;
        max-width: 190px;
    }

    .subItemCategory:hover span {
        color: #ffaa00;
    }

    .hoveredmainCategory {
        background-color: white !important;
    }

        .hoveredmainCategory .service-desc p {
            color: #ffaa00 !important;
        }

    @@media (max-width: 780px) {
        .custom-card-123 {
            max-width: 250px;
            width: 156px;
            margin: 6px !important;
        }

        .title-123 {
            font-size: 12px;
            margin-bottom: 4px;
        }

        .location-123 {
            font-size: 10px;
            margin-bottom: 1px;
        }

        .ayirma {
            font-size: 11px;
            margin-bottom: 1px;
        }

        .badge-123 {
            font-size: 10px;
            padding: 2px 5px;
        }

        .rateingNewAlt {
            font-size: 10px;
        }

        .rateingNew {
            font-size: 10px;
        }

        .custom-container-mainImage {
            max-width: 300px;
            max-height: 300px;
            display: none;
        }

            .custom-container-mainImage img {
                width: 150px;
                height: 150px;
                border-radius: 8px;
            }
    }

    .productAdsMain {
        border-radius: 10px;
        background-color: #f4f4f4;
        padding: 15px;
    }

    .ProductContentLayout img {
        border-top-left-radius: 10px;
        border-top-right-radius: 10px;
    }

    .ProductContentLayout {
        border-radius: 8px !important;
    }

    .custom-card-123 img {
        width: 150% !important;
        max-width: -webkit-fill-available;
        height: 100px !important;
        margin: -9px;
        margin-bottom: 10px;
        border-bottom-left-radius: 0px;
        border-bottom-right-radius: 0px;
    }

    @@media (max-width: 1200px) {
        .custom-card-123 {
            flex: 1 1 calc(40% - 50px);
            max-width: calc(50% - 20px);
            margin: 39px;
        }

        .carousel-innerNew {
            flex-wrap: wrap;
            display: flex;
            justify-content: center;
        }

        .productAdsMain {
            flex-wrap: wrap;
        }

        .ProductContentLayout {
            flex: 1 1 calc(40% - 50px);
        }

        .service-container {
            width: 100%;
            display: flex;
            align-items: center;
            flex-direction: column;
            justify-content: center;
        }

        .content-full {
            flex-wrap: wrap;
            width: fit-content;
        }

        #hizmetler li {
            flex: 1 1 calc(40% - 50px);
            width: 100px;
            height: 180px;
        }

        .servicseListing {
            position: relative;
            flex: 1 1 calc(40% - 50px);
        }

        #hizmetler li a {
            padding: 5px;
        }

        .service-desc {
            padding-top: 3px;
        }

        .services img {
            margin-right: 0px;
        }
    }



    .custom-card-123 {
        display: flex;
        flex-direction: column;
        align-items: center;
        max-width: 250px;
    }

    .servicseListing {
        list-style-type: none;
        padding: 10px;
        position: relative;
    }

    .ayirma div {
        padding-left: 25px;
    }
</style>

