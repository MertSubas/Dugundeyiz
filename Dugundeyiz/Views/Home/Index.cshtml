        let currentTipSlideIndex = -1;
    let firstTipSlideIndex = true;
    const tipSlides = document.querySelectorAll('.slider-slide');
    const tipDots = document.querySelectorAll('.dot');
    let slideInterval;

    // Slayt gösterimi için animasyon ve konum güncellemeleri
    function showTipSlide(index) {
        if (firstTipSlideIndex == true) {
            const newSlide = tipSlides[0];
            newSlide.style.display = 'block';
            newSlide.style.transition = 'none'; // Animasyon olmadan başlangıç pozisyonu ayarla
            newSlide.style.transform = (index > currentTipSlideIndex) ? 'translateX(100%)' : 'translateX(-100%)'; // Sağdan veya soldan başlasın

            // Tarayıcının slaytı güncellemesi için bir gecikme ekle
            setTimeout(() => {
                newSlide.style.transition = 'transform 0.5s ease'; // Yeni slaytın animasyonu
                newSlide.style.transform = 'translateX(0)'; // Hedef pozisyona geç
            }, 550); // 50ms gecikme ile yeni slaytı kaydırma animasyonu başlat

            // Dotları güncelle
            tipDots.forEach((dot, i) => {
                dot.classList.toggle('active', i === 0);
            });
            return
        }
        const currentSlide = tipSlides[currentTipSlideIndex];
        const newSlide = tipSlides[index];
        // Önceki slayt için kaybolma animasyonu
        if (currentSlide) {
            currentSlide.style.transition = 'transform 0.5s ease'; // Geçiş efektini ekle
            currentSlide.style.transform = ''; // Temizle

            // Yeni slayt sağdan gelecekse, mevcut slayt soldan çıksın
            if (index > currentTipSlideIndex) {
                currentSlide.style.transform = 'translateX(-100%)';
            } else {
                // Yeni slayt soldan gelecekse, mevcut slayt sağdan çıksın
                currentSlide.style.transform = 'translateX(100%)';
            }

            // Kaybolma animasyonu bittikten sonra slaytı gizle
            setTimeout(() => {
                currentSlide.style.display = 'none'; // Kaybolan slaytı gizle
                currentSlide.style.transform = ''; // Temizle
            }, 500); // Geçiş süresi kadar bekle (500ms)
        }

        // Yeni slaytı görünür yap ama animasyonsuz başla
        newSlide.style.display = 'block';
        newSlide.style.transition = 'none'; // Animasyon olmadan başlangıç pozisyonu ayarla
        newSlide.style.transform = (index > currentTipSlideIndex) ? 'translateX(100%)' : 'translateX(-100%)'; // Sağdan veya soldan başlasın

        // Tarayıcının slaytı güncellemesi için bir gecikme ekle
        setTimeout(() => {
            newSlide.style.transition = 'transform 0.5s ease'; // Yeni slaytın animasyonu
            newSlide.style.transform = 'translateX(0)'; // Hedef pozisyona geç
        }, 550); // 50ms gecikme ile yeni slaytı kaydırma animasyonu başlat

        // Dotları güncelle
        tipDots.forEach((dot, i) => {
            dot.classList.toggle('active', i === index);
        });

        currentTipSlideIndex = index; // Mevcut slaytın indexini güncelle
    }





    // Geçiş zamanlayıcısını başlat
    function startSlideInterval() {
        slideInterval = setInterval(() => {
            if (firstTipSlideIndex == true) {
                currentTipSlideIndex = 0;
                showTipSlide(currentTipSlideIndex + 1)
                firstTipSlideIndex = false;
            }
            else {
                let NewIndex = (currentTipSlideIndex + 1) % tipSlides.length;
                showTipSlide(NewIndex);
            }

        }, 15000); // 20 saniye
    }

    // Geçiş zamanlayıcısını sıfırlama
    function resetSlideInterval() {
        clearInterval(slideInterval);
        if (firstTipSlideIndex == true) {
            firstTipSlideIndex = false;
            let currentTipSlideIndex = 0;
        }
        startSlideInterval();
    }

    function nextTipSlide() {
        resetSlideInterval();
        let newIndex = (currentTipSlideIndex + 1) % tipSlides.length;
        showTipSlide(newIndex);
    }

    function prevTipSlide() {
        resetSlideInterval();
        let newIndex = (currentTipSlideIndex - 1 + tipSlides.length) % tipSlides.length;
        showTipSlide(newIndex);
    }

    function goToTipSlide(index) {
        resetSlideInterval();
        showTipSlide(index);
    }

    // Başlangıçta ilk slaytı göster ve zamanlayıcıyı başlat
    showTipSlide(currentTipSlideIndex);
    startSlideInterval();



</script>
